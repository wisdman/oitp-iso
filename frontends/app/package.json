  "description": "International School of Vasilieva",
  "private": true,
  "dependencies": {
    "@angular/common": "^8.0.0",
    "@angular/core": "^8.0.0",
    "@angular/forms": "^8.0.0",
    "@angular/platform-browser": "^8.0.0",
    "@angular/platform-browser-dynamic": "^8.0.0",
    "@angular/router": "^8.0.0",
    "core-js": "^3.1.3",
    "rxjs": "^6.5.2",
    "tslib": "^1.9.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.800.1",
    "@angular/compiler": "^8.0.0",
    "@angular/compiler-cli": "^8.0.0",
    "@angular/language-service": "^8.0.0",
    "@ngtools/webpack": "^8.0.1",
    "@types/autoprefixer": "^9.5.0",
    "@types/circular-dependency-plugin": "^5.0.0",
    "@types/compression-webpack-plugin": "^2.0.0",
    "@types/core-js": "^2.5.0",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/mini-css-extract-plugin": "^0.2.1",
    "@types/node": "^12.0.4",
    "@types/terser-webpack-plugin": "^1.2.0",
    "@types/webpack": "^4.4.32",
    "autoprefixer": "^9.5.1",
    "circular-dependency-plugin": "^5.0.2",
    "compression-webpack-plugin": "^2.0.0",
    "css-loader": "^2.1.1",
    "html-minifier": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.6.0",
    "postcss": "^7.0.16",
    "postcss-csso": "^3.0.0",
    "postcss-gap-properties": "^2.0.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^1.0.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.3.0",
    "ts-node": "^8.2.0",
    "typescript": "~3.4.0",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.5.1"
  },
  "scripts": {
    "postinstall": "find node_modules/{@types/**,terser,uglify-js}/node_modules -name 'source-map' -type d -exec rm -rf '{}' +",
    "clean": "rm -rf ./artifacts/*",
    "build": "npm run build-es2015 && npm run build-es5 && npm run build-rootdir",
    "build-es2015": "JS_TARGET='ES2015' TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' webpack --config ./webpack.config.ts",
    "build-es5": "JS_TARGET='ES5' TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' webpack --config ./webpack.config.ts",
    "build-rootdir": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' ts-node ./build-rootdir.ts",
    "start": "JS_TARGET='ES2015' TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' webpack-dev-server --config ./webpack.config.ts",
    "start-es5": "JS_TARGET='ES5' TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' webpack-dev-server --config ./webpack.config.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  }
}
